config_version=5

[application]

config/name="Heavy Metal Geomatrix"
run/main_scene="res://Scenes/Intro.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://AudioBus.tres"

[autoload]

Definitions="*res://Scripts/Managers/Definitions.gd"
AudioSettingsManager="*res://Scripts/Managers/AudioSettingsManager.gd"
VideoSettingsManager="*res://Scripts/Managers/VideoSettingsManager.gd"
InputSettingsManager="*res://Scripts/Managers/InputSettingsManager.gd"
